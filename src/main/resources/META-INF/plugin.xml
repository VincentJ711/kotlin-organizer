<idea-plugin>
  <id>com.github.vincentj711.kotlinorganizer</id>
  <name>kotlin-organizer</name>
  <vendor>vincentj711</vendor>

  <!-- Product and plugin compatibility requirements -->
  <!-- https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html -->
  <depends>com.intellij.modules.platform</depends>
  <depends>org.jetbrains.kotlin</depends>

  <actions>
    <action id="com.github.vincentj711.kotlinorganizer.actions.DfsSortAction"
            class="com.github.vincentj711.kotlinorganizer.actions.DfsSortAction"
            text="DFS Sort Kotlin Functions"/>
    <action id="com.github.vincentj711.kotlinorganizer.actions.BfsSortAction"
            class="com.github.vincentj711.kotlinorganizer.actions.BfsSortAction"
            text="BFS Sort Kotlin Functions"/>
  </actions>
</idea-plugin>
